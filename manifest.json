{
  "manifest_version": 3,
  "name": "Open Wazzup",
  "version": "1.0",
  "description": "Opens Wazzup page",
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["https://labelectro.bitrix24.ru/marketplace/app/22/"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["target.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["target.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
